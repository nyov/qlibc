prefix		= @prefix@
exec_prefix	= @exec_prefix@

## Compiler options
CC	= @CC@
CFLAGS	= @CFLAGS@
CPPFLAGS= -I../src/ @CPPFLAGS@
LIBS	= ../src/libqlibc.a @LIBS@

TARGETS	= list listtbl hashtbl hasharr vector vector_obj queue stack

## Main
all:	${TARGETS}

list: list.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ list.o ${LIBS}

listtbl: listtbl.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ listtbl.o ${LIBS}

hashtbl: hashtbl.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ hashtbl.o ${LIBS}

hasharr:	hasharr.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ hasharr.o ${LIBS}

vector: vector.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ vector.o ${LIBS}

vector_obj: vector_obj.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ vector_obj.o ${LIBS}

queue: queue.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ queue.o ${LIBS}

stack: stack.o
	${CC} ${CFLAGS} ${CPPFLAGS} -o $@ stack.o ${LIBS}

## Clear Module
clean:
	rm -f *.o ${TARGETS}

## Compile Module
.c.o:
	${CC} ${CFLAGS} ${CPPFLAGS} -c -o $@ $<
